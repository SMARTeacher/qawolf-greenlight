{
  "name": "@qawolf/ci-sdk",
  "version": "0.15.8",
  "description": "A simple SDK for interacting with QAWolf in CI scripts.",
  "type": "commonjs",
  "private": false,
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "types": "./dist/types/index.d.ts",
  "main": "cjs/index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "lint": "eslint . --ext js,jsx,mjs,ts,tsx --max-warnings=0 && prettier --check .",
    "lint:fix": "eslint . --ext js,jsx,mjs,ts,tsx --fix --max-warnings=0 && prettier --write .",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "npm-publish": "./scripts/publish.sh",
    "tsc:check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Jules Sam. Randolph<jules@qawolf.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@sinonjs/fake-timers": "^10.3.0",
    "@tsconfig/strictest": "^2.0.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.0",
    "typescript-transformer-esm": "^1.1.0"
  },
  "dependencies": {
    "@qawolf/ci-utils": "^1.0.0",
    "slugify": "^1.6.6",
    "tslib": "^2.6.2"
  }
}
